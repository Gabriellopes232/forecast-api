{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Complete API built with Node.js and Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:local": "ts-node-dev --transpile-only 'src/index.ts' | pino-pretty -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --forceExit --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabriellopes232/forecast-api.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Gabriel Lopes do Nascimento",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gabriellopes232/forecast-api/issues"
  },
  "homepage": "https://github.com/Gabriellopes232/forecast-api#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/express": "^4.17.14",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "config": "^3.3.8",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.7.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^3.3.0",
    "@types/jest": "^29.2.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/nock": "^11.1.0",
    "@types/node": "^18.11.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}